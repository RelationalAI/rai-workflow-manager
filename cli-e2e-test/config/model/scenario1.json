{
  "workflow": [
    {
      "type": "ExecuteCommand",
      "name": "TestExecuteCommand",
      "command": "echo 'Running ExecuteCommand!' \n mkdir test \n ls test \n python scripts/test_script.py \n ./scripts/test_script.sh"
    },
    {
      "type": "ConfigureSources",
      "name": "ConfigureSources",
      "configFiles": [
        "config/config1.rel"
      ],
      "defaultContainer": "input",
      "sources": [
        {
          "relation": "zip_city_state_master_data",
          "relativePath": "master_source/zip_city_state",
          "inputFormat": "csv"
        },
        {
          "relation": "device_data",
          "isChunkPartitioned": true,
          "isDatePartitioned": true,
          "relativePath": "device",
          "inputFormat": "csv",
          "loadsNumberOfDays": 60
        },
        {
          "relation": "store_data",
          "isChunkPartitioned": true,
          "isDatePartitioned": true,
          "relativePath": "store",
          "inputFormat": "jsonl",
          "extensions": [
            "json",
            "jsonl"
          ],
          "loadsNumberOfDays": 60
        },
        {
          "relation": "product_data",
          "isChunkPartitioned": true,
          "isDatePartitioned": false,
          "relativePath": "product",
          "inputFormat": "csv"
        },
        {
          "relation": "user_data",
          "isChunkPartitioned": false,
          "isDatePartitioned": true,
          "relativePath": "user",
          "inputFormat": "csv",
          "loadsNumberOfDays": 10
        }
      ]
    },
    {
      "type": "InstallModels",
      "name": "InstallModels1",
      "needs": ["TestExecuteCommand", "ConfigureSources"],
      "modelFiles": [
        "device.rel",
        "zip.rel",
        "store.rel",
        "user.rel",
        "product.rel",
        "json_schema_mapping.rel",
        "device_seen.rel"
      ]
    },
    {
      "type": "LoadData",
      "name": "LoadData",
      "needs": ["InstallModels1"]
    },
    {
      "type": "Materialize",
      "name": "Materialize",
      "needs": ["LoadData"],
      "relations": [
        "city:name",
        "device:name",
        "store:name",
        "product:name",
        "user:name"
      ],
      "materializeJointly": true
    },
    {
      "type": "Export",
      "name": "Export",
      "needs": ["Materialize"],
      "exportJointly": true,
      "dateFormat": "%Y%m%d",
      "defaultContainer": "export",
      "exports": [
        {
          "type": "csv",
          "configRelName": "cities_csv",
          "relativePath": "cities"
        },
        {
          "type": "csv",
          "configRelName": "devices_csv",
          "relativePath": "devices"
        },
        {
          "type": "csv",
          "configRelName": "products_csv",
          "relativePath": "products"
        },
        {
          "type": "csv",
          "configRelName": "stores_csv",
          "relativePath": "stores"
        },
        {
          "type": "csv",
          "configRelName": "users_csv",
          "relativePath": "users"
        }
      ]
    }
  ]
}
