{
  "workflow": [
    {
      "type": "ConfigureSources",
      "name": "ConfigureSources",
      "configFiles": [
        "config/config3.rel"
      ],
      "defaultContainer": "input",
      "sources": [
        {
          "relation": "city_data",
          "isChunkPartitioned": true,
          "isDatePartitioned": true,
          "relativePath": "temp/city",
          "inputFormat": "csv",
          "loadsNumberOfDays": 60
        }
      ]
    },
    {
      "type": "InstallModels",
      "name": "InstallModels1",
      "needs": ["ConfigureSources"],
      "modelFiles": [
        "city.rel"
      ]
    },
    {
      "type": "LoadData",
      "name": "LoadData",
      "needs": ["InstallModels1"]
    },
    {
      "type": "Materialize",
      "name": "Materialize",
      "needs": ["LoadData"],
      "relations": [
        "city:name"
      ],
      "materializeJointly": true
    },
    {
      "type": "Export",
      "name": "Export",
      "needs": ["Materialize"],
      "exportJointly": false,
      "dateFormat": "%Y%m%d",
      "defaultContainer": "export",
      "exports": [
        {
          "type": "csv",
          "configRelName": "cities_csv",
          "relativePath": "cities"
        }
      ]
    }
  ]
}
