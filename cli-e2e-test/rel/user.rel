module import_config[:user_data]
    def schema {
        (:user_id, "int") ;
        (:user_name, "string")
    }

    declare syntax(:header, __trail...) requires true
end

module user_data
    def USER_NAME[idx, row]: source_catalog[:user_data, idx, :user_name, row]
end

module user
    def name { user_data[:USER_NAME, _, _] }
end